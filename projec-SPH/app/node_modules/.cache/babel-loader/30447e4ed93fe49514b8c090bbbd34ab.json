AAA,CAAA;AACA,UAAA,IAAA,IAAA,IAAA,CAAA,WAAA,EAAA,IAAA,GAAA,IAAA,CAAA,WAAA;AACA,UAAA,GAAA,IAAA,CAAA,EAAA,GAAA,GAAA,CAAA;AACA,UAAA,GAAA,IAAA,IAAA,CAAA,YAAA,EAAA,GAAA,GAAA,IAAA,CAAA,YAAA,CATA,CAUA;;AACA,MAAA,IAAA,CAAA,KAAA,CAAA,IAAA,GAAA,IAAA,GAAA,IAAA;AACA,MAAA,IAAA,CAAA,KAAA,CAAA,GAAA,GAAA,GAAA,GAAA,IAAA;AACA,MAAA,GAAA,CAAA,KAAA,CAAA,IAAA,GAAA,CAAA,CAAA,GAAA,IAAA,GAAA,IAAA;AACA,MAAA,GAAA,CAAA,KAAA,CAAA,GAAA,GAAA,CAAA,CAAA,GAAA,GAAA,GAAA,IAAA;AACA;AAhBA;AAnBA,CAAA","sourcesContent":["<template>\n  <div class=\"spec-preview\">\n    <img :src=\"skuImgeList.imgUrl\" />\n    <div class=\"event\" @mousemove=\"handler\"></div>\n    <div class=\"big\">\n      <img :src=\"skuImgeList.imgUrl\" ref=\"big\" />\n    </div>\n    <div class=\"mask\" ref=\"mask\"></div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"Zoom\",\n  props: [\"skuImgList\"],\n  data() {\n    return {\n      currentIndex: 0,\n    };\n  },\n  computed: {\n    skuImgeList() {\n      return this.skuImgList[this.currentIndex] || {};\n    },\n  },\n  mounted() {\n    //全局事件总线，获取兄弟组件传递过来的索引值\n    this.$bus.$on(\"getIndex\", (index) => {\n      this.currentIndex = index;\n    });\n  },\n  methods: {\n    handler(event) {\n      let mask = this.$refs.mask;\n      let big = this.$refs.big;\n      let left = event.offsetX - mask.offsetWidth / 2;\n      let top = event.offsetY - mask.offsetHeight / 2;\n      //约束范围\n      if (left <= 0) left = 0;\n      if (left >= mask.offsetWidth) left = mask.offsetWidth;\n      if (top <= 0) top = 0;\n      if (top >= mask.offsetHeight) top = mask.offsetHeight;\n      //修改元素的left|top属性值\n      mask.style.left = left + \"px\";\n      mask.style.top = top + \"px\";\n      big.style.left = -2 * left + \"px\";\n      big.style.top = -2 * top + \"px\";\n    },\n  },\n};\n</script>\n\n<style lang=\"less\">\n.spec-preview {\n  position: relative;\n  width: 400px;\n  height: 400px;\n  border: 1px solid #ccc;\n\n  img {\n    width: 100%;\n    height: 100%;\n  }\n\n  .event {\n    width: 100%;\n    height: 100%;\n    position: absolute;\n    top: 0;\n    left: 0;\n    z-index: 998;\n  }\n\n  .mask {\n    width: 50%;\n    height: 50%;\n    background-color: rgba(0, 255, 0, 0.3);\n    position: absolute;\n    left: 0;\n    top: 0;\n    display: none;\n  }\n\n  .big {\n    width: 100%;\n    height: 100%;\n    position: absolute;\n    top: -1px;\n    left: 100%;\n    border: 1px solid #aaa;\n    overflow: hidden;\n    z-index: 998;\n    display: none;\n    background: white;\n\n    img {\n      width: 200%;\n      max-width: 200%;\n      height: 200%;\n      position: absolute;\n      left: 0;\n      top: 0;\n    }\n  }\n\n  .event:hover ~ .mask,\n  .event:hover ~ .big {\n    display: block;\n  }\n}\n</style>"],"sourceRoot":"src/pages/Detail/Zoom"}]}