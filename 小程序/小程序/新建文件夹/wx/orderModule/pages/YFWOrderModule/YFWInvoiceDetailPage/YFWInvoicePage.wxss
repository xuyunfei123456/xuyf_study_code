.top_bg {
  position: fixed;
  width: 100%;
  height: 340rpx;
  top: 0;
  z-index: -1;
}

.invoice-content {
  width: 91vw;
  margin: 34rpx 3vw 0;
  padding: 20rpx 1.5vw;
  border-radius: 14rpx;
  background-color: #fff;
  box-shadow: 0rpx 6rpx 20rpx rgba(204, 204, 204, 0.5);
}

.invoice-content image {
  width: 91vw;
  height: 430rpx;
}