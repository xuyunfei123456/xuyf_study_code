  flex-direction: row;
  justify-content: center;
  align-content: center;
  font-size: 26rpx;
  color: #999;
}

.onloading .load-wrap .loading-icon {
  animation: onloading-view linear 1.6s 0s infinite;
  -webkit-animation: onloading-view linear 1.6s 0s infinite;
  display: inline-block;
  height: 30rpx;
  width: 30rpx;
  position: relative;
  border-radius: 50%;
  border: 5rpx solid #0ba756;
}

.onloading .load-wrap .loading-icon .hide-block {
  display: inline-block;
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  top: 0;
  left: 40%;
  background-color: #f5f5f5;
}

