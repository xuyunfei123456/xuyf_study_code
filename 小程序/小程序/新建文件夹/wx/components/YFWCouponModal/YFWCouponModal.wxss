/* components/YFWCouponModal/YFWCouponModal.wxss */
.modal-content {
  height: 100%;
  width: 100%;
}

.modal-title {
  font-size: 28rpx;
  color: #000;
  margin-left: 26rpx;
}

.modal-coupon-view {
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
  margin-bottom: 40rpx;
  padding: 0 26rpx;
}

.modal-coupon-left {
  width: 32%;
  height: 200rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  background: #f8f8f8 no-repeat center center;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,)
}

.modal-coupon-unit {
  color: #fff;
  font-size: 42rpx;
  font-weight: bold;
  line-height: 42rpx;
  margin-top: 30rpx;
}

.modal-coupon-amount {
  color: #fff;
  font-size: 84rpx;
  font-weight: bold;
  line-height: 84rpx;
}

.modal-coupon-right {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-start;
  padding: 30rpx;
  width: 64%;
  background-color: #fff;
}

.modal-coupon-top-view {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.modal-coupon-type {
  font-size: 24rpx;
  line-height: 24rpx;
  border: solid 2rpx #1fdb9b;
  padding: 4rpx 12rpx;
  margin-right: 10rpx;
  border-radius: 20rpx;
  color: #1fdb9b;
}

.modal-coupon-title {
  font-size: 28rpx;
  font-weight: bold;
  color: #333;
}

.modal-coupon-bottom-view {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  width: 100%;
}

.modal-coupon-time {
  font-size: 24rpx;
  color: #333;
}

.modal-coupon-receiveing {
  color: #fff;
  font-size: 24rpx;
  padding: 8rpx 20rpx;
  border-radius: 30rpx;
  background: linear-gradient(to left, #00c891,#00df96);
  box-shadow: -6rpx 6rpx 30rpx rgba(31,219,155,0.5);
}

.modal-coupon-received {
  color: #1fdb9b;
  font-size: 24rpx;
  padding: 8rpx 20rpx;
  line-height: 24rpx;
  border-radius: 30rpx;
  border: solid 2rpx #1fdb9b;
}
.detail-padding-13 {
  padding: 26rpx;
}
.detail-column-view {
  flex-direction: column;
  display: flex;
  padding-bottom: 30rpx
}
.detail-row-view {
  flex-direction: row;
  display: flex;
}
.detail-margin-bottom-15 {
  margin-bottom: 40rpx;
}
.detail-align-start {
  align-items: flex-start;
}
.detail-discount-title {
  font-size: 20rpx;
  line-height: 26rpx;
  text-align: center;
  border-radius: 16rpx;
  color: #fff;
  margin-right: 10rpx;
  min-width: 60rpx;
}
.detail-discount-back-yellow {
  background: linear-gradient(to right, #faab81,#fad16e);
}

.detail-discount-back-blue {
  background: linear-gradient(to right, #2c5cf1,#7c64f7);
}
.detail-dark-text {
  color: #333;
}
.detail-13-text {
  font-size: 26rpx;
  line-height: 26rpx;
}
.detail-title {
  font-size: 28rpx;
  line-height: 22rpx;
  color: #999;
  padding-bottom: 40rpx;
}