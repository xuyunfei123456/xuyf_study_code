/* component/ListGoodsItem.wxss */
.listItemView{
  display: flex;
  width: 100vw;
  align-items: center;
  background-color: white;
}
.listItemView .leftView{
  display: flex;
  height: 200rpx;
  width: 26%;
  justify-content: center;
  align-items: center;
}
.listItemView .rightView{
  display: flex;
  flex-direction: column;
  width: 70%;
  justify-content: center;
  padding-bottom: 20rpx;
  border-bottom: solid 2rpx #f7f7f7;
}
.itemImage{
  width: 140rpx;
  height: 140rpx;
}
.medicineName{
  color: #333333;
  font-size: 30rpx;
  margin-top: 30rpx;
  margin-right: 20rpx;
    /* 显示两行： */
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
/* 多出部分显示省略号... */
  overflow: hidden;
  text-overflow: ellipsis;
  float: left;
}
.medicineName text, image {
  vertical-align: middle;
  display: inline-block;
}
.medicineName .medicine_ac_url {
  width: 56rpx;
  height: 24rpx;
  margin-bottom: 6rpx;
}
.medicineStandards{
  color: #666666;
  font-size: 24rpx;
  margin-top: 2rpx;
}
.medicineAuthorizedCode{
  color: #999999;
  font-size: 24rpx;
  margin-top: 2rpx;
}
.medicine-type {
  color: #999999;
  font-size: 32rpx;
}
.itemRightBottomView{
  display: flex;
  flex-direction: row;
  margin-top: 12rpx;
  left: 0rpx;
  right: 0rpx;
  height: 40rpx;
  justify-content:space-between;
}
.priceView{
  display: flex;
  align-items: center;
  flex-direction: row;
}
.itemPrice{
  color: #FF3300;
  font-size: 34rpx;
}
.carView{
  position: absolute;
  right: 20rpx;
  display: flex;
  padding: 0 20rpx;
}
.addCar{
  width: 40rpx;
  height: 40rpx;
}
.labelView{
  display:flex;
  border: solid #FF3B00 1rpx;
  border-radius: 10rpx;
  margin-left: 13rpx;
}
.labelText{
  display:block;
  font-size:20rpx;
  color:#FF3B00;
  text-align:center;
  line-height:14rpx;
  margin:7rpx 2rpx 7rpx 2rpx;
  height:14rpx;
}
.store{
  position: absolute;
  right: 20rpx;
  font-size: 24rpx;
  text-align: left;
  font-family: "宋体";
  color: #999999;
}
.storetext{
  color: #1fdf9b;
}
.shop_container {
  flex-direction: row;
  display: flex;
  align-items: center;

}
.shop_default {
  min-width: 46rpx;
  min-height: 22rpx;
  background-color: #379dd8;
  border-radius: 16rpx;
  color: white;
  font-size: 16rpx;
  padding:2rpx 10rpx;
  text-align:center;
}
.shop_title {
  color: #666;
  font-size: 24rpx;
  flex: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-left: 4rpx;
}
.shop_more {
  color: #1fdf9b;
  font-size: 24rpx;
}
.shop_arrow {
  width: 12rpx;
  height: 22rpx;
  margin-left: 4rpx;
}
.shop_car {
  width: 44rpx;
  height: 44rpx;
  margin-top: -10rpx;
}
.activity{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  padding-top: 16rpx;
  align-items: center;
}
.freeExpress{
  background:linear-gradient(to right,rgb(255,154,103),rgb(255,96,94));
  color: white;
  padding: 6rpx 16rpx;
  border-radius: 12rpx;
  font-size: 22rpx;
}
.coupon{
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  padding: 0px 10px;
  color: #ff3300;
  margin-left: 10rpx;
}
.fxcss{ 
  border: 1px solid rgb(255,96,94);
  border-radius: 12rpx;
  padding: 0px 8px;
  color: #ff3300;
  font-size: 22rpx;
}
