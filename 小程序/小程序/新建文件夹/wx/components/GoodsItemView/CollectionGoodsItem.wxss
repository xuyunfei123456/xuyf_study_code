/* component/collectionGoodsItem.wxss */
.collectionItemView{
  width: 100%;
  height: auto;
  border: 2rpx solid #cecece47;
  border-radius: 20rpx;
  box-shadow: 0px 0px 30rpx #cecece47;
  padding-bottom: 32rpx;
  background-color: white;
}
.imageView{
  margin: 20rpx auto;
  /* border: 1rpx solid yellow; */
  display: flex;
  justify-content: center;
}
.image{
  width: 265rpx;
  height: 265rpx;
  
}
.nameView{
  display: flex;
  height: 80rpx;
  align-items: center;
}
.name{
  /* width: 100%; */
  text-align: left;
  font-family: "宋体";
  font-size: 30rpx;
  color: "#333333";
  /* border: 1rpx solid green; */
  margin: 0 15rpx;
  margin-top: 20rpx;
  /* 显示两行： */
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
/* 多出部分显示省略号... */
  overflow: hidden;
  text-overflow: ellipsis;
}
.name text, image{
  vertical-align: middle;
  display: inline-block;
}
.name .name_ac_url {
  width: 56rpx;
  height: 24rpx;
  margin-bottom: 6rpx;
}
.standards{
   /* width: 100%; */
  margin: 0 15rpx;
  margin-top: 12rpx;
  font-size: 24rpx;
  text-align: left;
  font-family: "宋体";
  color: #999999;
}
.bottomView{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin: 0 15rpx;
  margin-top: 20rpx;
}
.priceView{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items:center;
}
.bottomView .rightview{
  display: flex;
  align-items: center;
  flex-direction: row;
}
.price{
  text-align: left;
  font-family: "SourceHanSansCN-Medium";
  font-size: 34rpx;
  color:#FF3B00;
}

.discountView{
  display:flex;
  border: solid #FF3B00 1rpx;
  border-radius: 10rpx;
  margin-left: 13rpx;
}
.discount{
  display:block;
  font-size:20rpx;
  color:#FF3B00;
  text-align:center;
  line-height:14rpx;
  margin:7rpx 2rpx 7rpx 2rpx;
  height:14rpx;
}
.freeView{
  display:flex;
  border: solid #1fdf9b 1rpx;
  border-radius: 10rpx;
}
.free{
  display:block;
  font-size:20rpx;
  color:#1fdf9b;
  text-align:center;
  line-height:14rpx;
  margin:7rpx 2rpx 7rpx 2rpx;
  height:14rpx;
}
.car{
  width: 40rpx;
  height: 40rpx;
}
.carView{
  display: flex;
  padding: 0 20rpx;
}
.store{
  margin: 0 15rpx;
  margin-top: 10rpx;
  font-size: 24rpx;
  text-align: left;
  font-family: "宋体";
  color: #999999;
}
.storetext{
  color: #1fdf9b;
}
.shop_container {
  flex-direction: row;
  display: flex;
  align-items: center;

}
.shop_default {
  background-color: #379dd8;
  border-radius: 16rpx;
  color: white;
  font-size: 16rpx;
  padding:2rpx 10rpx;
  text-align:center;
  margin-left: 10rpx;
}
.shop_title {
  color: #666;
  font-size: 24rpx;
  flex: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-left: 4rpx;
}
.shop_more {
  color: #1fdf9b;
  font-size: 24rpx;
}
.shop_arrow {
  width: 12rpx;
  height: 22rpx;
  margin-left: 4rpx;
}
.shop_car {
  width: 44rpx;
  height: 44rpx;
}

.medicine-type {
  color: #999999;
  font-size: 22rpx;
}
.activity{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  padding-top: 16rpx;
  align-items: center;
  margin: 0rpx 15rpx;
}
.freeExpress{
  background:linear-gradient(to right,rgb(255,154,103),rgb(255,96,94));
  color: white;
  padding: 6rpx 16rpx;
  border-radius: 12rpx;
  font-size: 22rpx;
}
.coupon{
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  padding: 0px 10px;
  color: #ff3300;
  margin-left: 10rpx;
}
.fxcss{ 
  border: 1px solid rgb(255,96,94);
  border-radius: 12rpx;
  padding: 0px 8px;
  color: #ff3300;
  font-size: 22rpx;
}
.listitem{
  color: #999999;
  font-size: 24rpx;
  margin-left:15rpx;
  margin-top:12rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.freeShip{
  color: #fefefe;
  background: linear-gradient(to right, #ff9a67, #ff605e);
  font-size: 20rpx;
  border-radius: 10rpx;
  padding: 6rpx 10rpx;
}
.couponitem{
  margin-left: 10rpx;
  padding: 4rpx 8rpx;
  color: #ff3300;
  letter-spacing: 2px;
  text-align: center;
  font-size: 20rpx;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,);
}
.couponAndFreeship{
  display: flex;
  flex-direction: row;
  margin-left: 15rpx;
}