.container {
  width: 100vw;
  height: 100%;
}
.scroll {
  width: 100%;
  height: 100%;
  -webkit-overflow-scrolling: touch;
}
.topView{
  width: 100%;
  height: 106rpx;
  flex-direction: column;
}
.topView-context {
  width: 100%;
  height: 104rpx; 
  flex-direction: row;
  align-items: center;
  display: flex;
}
.topView-context image{
  width: 36rpx;
  height: 36rpx;
  margin-left: 26rpx;
}
.topView-context text{
  font-size: 30rpx;
  color: #666666;
  margin-left: 18rpx;
  
}
.lineview {
  width: 100%;
  height: 2rpx;
  background-color: #f5f5f5;
}
.list-view {
  width: 100%;
  height: 88rpx;
  flex-direction: column;
}
.list-context{
  width: 100%;
  height: 86rpx;
  display: flex;
  align-items: center;
}
.list-context text {
  margin-left: 26rpx;
  font-size: 30rpx;
  color: #333333
}