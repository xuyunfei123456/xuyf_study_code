.container {
  width: 100vw;
  height: 100%;
}
.scroll {
  width: 100%;
  height: 100%;
  display: flex;
  -webkit-overflow-scrolling: touch;
}
.no-result{
  margin: 0rpx 20rpx;
  height: 532rpx;
  display: flex;
  flex-direction: column;
  background-color: white;
  box-shadow: 0rpx 8rpx 8rpx 0rpx rgba(204, 204, 204, 0.4);
  border-radius: 14rpx;
  justify-content: center;
  align-items: center

}
.no-result image {
  width: 276rpx;
  height: 170rpx;
}
.no-result text {
  margin-top: 40rpx;
  font-size: 26rpx;
  color: #999999
}
.about-shop-view {
  height: 80rpx;
  margin: 10rpx 20rpx 0rpx 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.result-view{
  margin: 0rpx 20rpx 20rpx 20rpx;
  flex-direction: column;
  background-color: white;
  box-shadow: 0rpx 8rpx 8rpx 0rpx rgba(204, 204, 204, 0.4);
  border-radius: 14rpx;
  display: flex;
  flex: 1;
}
.topView {
  width: 100%;
  height: 160rpx;
  flex-direction: row;
  align-items: center;
  display: flex;
  justify-content: flex-start
}
.result-view .logo-url{
  margin: 50rpx 16rpx 50rpx 40rpx;
  width: 132rpx;
  height: 60rpx;
  border: 1rpx solid #eeeeee
}

.topView .title{
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  line-height: 30rpx;
  font-size: 26rpx;
  color: #333333;
  width: 42vw;

}
.kind {
  flex: 1
}
.clickshop {
  width: 17%;
  height: 46rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-right: 26rpx;
  border-radius: 23rpx;
  border: 2rpx solid #26af73;
}
.clickshop text {
  font-size: 24rpx;
  color: #26af73
}
.context {
  display: flex;
  flex: 1;
  margin: 2rpx 28rpx 40rpx 40rpx;
  flex-direction: row
}
.goods-view {
  height: 260rpx;
  width: 33%;
  margin-right: 16rpx;
  display: flex;
  flex: 1;
  flex-direction: column;
}
.goods-view image{
  width: 100%;
  height: 180rpx;
}
.goods-view-title {
  font-size: 22rpx;
  color: #333333;
  margin-top: 24rpx;
  flex: 1;
  /* width: 100%;
  height: 24rpx;
  overflow: hidden; 
  text-overflow: ellipsis;
  white-space: nowrap;  */

}
.goods-view-price {
  margin-top: 14rpx;
  display: flex;
}
.onloading {
  margin-left: -14rpx;
  border-top: 3rpx solid #f3f3f3;
  text-align: center;
  font-size: 0;
  padding: 20rpx 0;
  background-color: #ffffff;
}
.onloading .load-wrap .loading-icon {
  animation: onloading-view linear 1.6s 0s infinite;
  -webkit-animation: onloading-view linear 1.6s 0s infinite;
  display: inline-block;
  height: 23px;
  width: 23px;
  position: relative;
  border-radius: 50%;
  border: 5rpx solid #0BA756;
}
.onloading .load-wrap .loading-icon .hide-block {
  display: inline-block;
  position: absolute;
  width: 20px;
  height: 20px;
  top: 0;
  left: 40%;
  background-color: #ffffff;
}
.onloading .load-wrap .loadover {
  font-size: 24rpx;
  color: #999999;
}