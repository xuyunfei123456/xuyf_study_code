<!--pages/YFWGoodsDetailModule/YFWGoodsDetailPage/YFWGoodsDetail.wxml-->
<!-- 空白view -->
<view wx:if="{{isShowWhite}}" class='detail-white-view'></view>
<view class='detail-view'>
  <!-- 购物车、更多 -->
  <view wx:if="{{dataSource.isShowTopItem}}" class="top-view" id="topTabView">
    <view class='top-lr-width'></view>
    <view hidden='{{!dataSource.isShowTopItem}}' class='top-center-view'>
      <view class='top-center-item' bindtap='topViewIndexChange' data-index='0'>
        <view class="{{dataSource.topIndex==0 ? 'top-center-title-select' : 'top-center-title-normal'}}">商品</view>
        <view class="{{dataSource.topIndex==0 ? 'top-center-line-select' : 'top-center-line-normal'}}"></view>
      </view>
      <view class='top-center-item' bindtap='topViewIndexChange' data-index='1'>
        <view class="{{dataSource.topIndex==1 ? 'top-center-title-select' : 'top-center-title-normal'}}">评价</view>
        <view class="{{dataSource.topIndex==1 ? 'top-center-line-select' : 'top-center-line-normal'}}"></view>
      </view>
      <view class='top-center-item' bindtap='topViewIndexChange' data-index='2'>
        <view class="{{dataSource.topIndex==2 ? 'top-center-title-select' : 'top-center-title-normal'}}">详情</view>
        <view class="{{dataSource.topIndex==2 ? 'top-center-line-select' : 'top-center-line-normal'}}"></view>
      </view>
    </view>
    <view class='top-right-view top-lr-width'>
      <view class='top-right-view-item' bindtap='lookShopCartDetail'>
        <image lazy-load="{{true}}"  src='/images/YFWGoodsDetailModule/goods_deail_cart.png' class='top-view-image' mode='aspectFit'></image>
        <view wx:if="{{dataSource.shop_cart_count > 0}}" class='top-view-cart'>
          {{dataSource.shop_cart_count > 99 ? "99+" : dataSource.shop_cart_count }}</view>
      </view>
      <view class='top-right-view-item' bindtap='lookMoreView' id='more_image'>
        <image lazy-load="{{true}}"  src='/images/YFWGoodsDetailModule/goods_deail_more.png' class='top-view-image' mode="aspectFit"></image>
      </view>
    </view>
  </view>
  <view wx:else class="top-view-opa" id="topTabView">
    <view class='top-lr-width'></view>
    <view class='top-right-view top-lr-width'>
      <view class='top-right-view-item' bindtap='lookShopCartDetail'>
        <image lazy-load="{{true}}"  src='/images/YFWGoodsDetailModule/goods_deail_cart.png' class='top-view-image' mode='aspectFit'></image>
        <view wx:if="{{dataSource.shop_cart_count > 0}}" class='top-view-cart'>
          {{dataSource.shop_cart_count > 99 ? "99+" : dataSource.shop_cart_count }}</view>
      </view>
      <view class='top-right-view-item' bindtap='lookMoreView' id='more_image'>
        <image lazy-load="{{true}}"  src='/images/YFWGoodsDetailModule/goods_deail_more.png' class='top-view-image' mode="aspectFit"></image>
      </view>
    </view>
  </view>
  <!-- 图片 -->
  <view class='swiper-content'>
    <swiper class='swiper-content-swiper' circular='true' bindchange='medicineImageChangeIndex'>
      <block wx:for='{{dataSource.medicine_images}}' wx:key='index'>
        <swiper-item>
          <image lazy-load="{{true}}"  src='{{item}}' class='swiper-content-item' mode='aspectFit' bindtap='showBigImage'
            data-images='{{dataSource.medicine_images}}' data-current='{{item}}' data-type='medicine'></image>
        </swiper-item>
      </block>
    </swiper>
    <view class='swiper-content-number'>{{dataSource.medicine_image_index}}</view>
  </view>
  <!-- 品质保障、提供发票 -->
  <scroll-view scroll-x class='detail-service-view'>
    <block wx:for='{{dataSource.medicine_services}}' wx:key='index'>
      <view class='detail-image-title-view detail-service-iitem detail-dark-text detail-12-text'
        bindtap="showReturnPayModal" data-item='{{item}}'>
        <image lazy-load="{{true}}"  class='detail-service-icon' mode='widthFix' src='/images/YFWGoodsDetailModule/goods_deail_service.png'>
        </image>
        {{item}}
        <image lazy-load="{{true}}"  wx:if="{{item == '退货款规则'}}" class="detail-arrow-icon" mode='widthFix'
          src='/images/icon_arrow_more.png' />
      </view>
    </block>
  </scroll-view>
  <!-- 商品价格、名称、疗效 -->
  <view class='detail-column-view'>
    <view class='detail-price'>
      <priceView price="{{dataSource.medicine_info.price}}" fontSize="38"
        discount="{{dataSource.medicine_info.discount}}"></priceView>
      <image lazy-load="{{true}}"  wx:if="{{dataSource.is_seckill}}" class="detail-miaosha" src="/images/miaosha.png"></image>
      <view class="detail-empty-flex"></view>
      <!-- 降价通知 -->
      <!-- <view bindtap="jumpDiscountNotice" class="detail-low-content" data-price="{{dataSource.medicine_info.price}}">
        <image lazy-load="{{true}}"  src='/images/jjtz_icon.png'/>
        <text>降价通知</text>
      </view> -->
      <!-- <image lazy-load="{{true}}"  src='/images/YFWGoodsDetailModule/goods_deail_share.png' class='detail-price-share'></image>
      <button class='detail-price-share-button' open-type='share'></button> -->
    </view>
    <!-- 优惠劵、促销 -->
    <view class="detail-column-view"
      wx:if="{{dataSource.medicine_coupons.length>0 || dataSource.medicine_discount.length>0 ||dataSource.medicine_freepostage_list.length>0}}">
      <view
        wx:if="{{dataSource.medicine_coupons.length>0 || dataSource.medicine_discount.length>0 ||dataSource.medicine_freepostage_list.length>0}}"
        class="detail-coupon-content-view" bindtap="showCouponModal">
        <view class='detail-coupon-itemview'>
          <view wx:if="{{dataSource.medicine_coupons.length>0}}" class='detail-discount-coupon'>
            <text style="font-size: 24rpx; color: #FF6363; text-align: center; margin-right: 2rpx;">劵</text>
            <view class="detail-coupon-title" />
            <text
              style="font-size: 24rpx; color: #FF6363; margin-left: 3rpx">{{dataSource.medicine_coupons[0].name}}</text>
          </view>
          <view wx:if="{{dataSource.medicine_show_discount.length>0}}" class='detail-discount-activity'>
            {{dataSource.medicine_show_discount[0].name}}</view>
          <view wx:if="{{dataSource.medicine_freepostage_list.length>0}}" class='detail-discount-freepostage'>
            <text>{{dataSource.medicine_freepostage_list[0].name}}</text>
          </view>
        </view>
        <view class="detail-check-view">
          <text style="font-size: 28rpx; color: #999;">{{dataSource.medicine_coupons.length==0?'包邮说明':'领劵'}}</text>
          <image lazy-load="{{true}}"  src='/images/around_detail_icon.png' class='detail-detail-icon'></image>
        </view>

      </view>
    </view>
    <view class='detail-column-view detail-medicine-info-view'>
      <image lazy-load="{{true}}" src='/images/YFWGoodsDetailModule/goods_deail_share.png' class='detail-price-share'></image>
      <button class='detail-price-share-button' open-type='share'></button>
      <view class='detail-image-title-view detail-dark-text detail-15-text detail-bold-text detail-margin-right-130'>
        <image lazy-load="{{true}}"  wx:if="{{dataSource.medicine_info.medicine_type != -1 && dataSource.medicine_info.medicine_icon}}"
          src='{{dataSource.medicine_info.medicine_icon}}' class="detail-medicine-icon" mode='widthFix'></image>
        <image lazy-load="{{true}}"  wx:if="{{dataSource.medicine_info.activity_img_url.length>0}}"
          src='{{dataSource.medicine_info.activity_img_url}}' class="detail-medicine-icon" mode='widthFix'></image>
        {{dataSource.medicine_info.medicine_name}}
        
      </view>
      <wxs module="ellipsis" src="/wxs/ellipsis.wxs"></wxs>
      <view 
        style="margin-top: 40rpx" 
        wx:if="{{dataSource.medicine_info.medicine_indications.length>0}}"
        class='detail-dark-text detail-12-text detail-medicine-indications'>
        {{hasSpecification ? ellipsis(dataSource.medicine_info.medicine_indications, 45) : dataSource.medicine_info.medicine_indications}}
        <view wx:if="{{hasSpecification}}" style="display: inline-block" bindtap="showGuideModal">
          <text class="detail-medicine-indications-controls">查看说明书</text>
          <image 
          lazy-load="{{true}}"  
          src="../../../images/YFWGoodsDetailModule/goods_deail_arrow_red.png" 
          style="vertical-align: middle;width:12rpx;height:22rpx;margin-left:10rpx"></image>
          </view>
        </view>
      <view 
        wx:if="{{dataSource.medicine_info.medicine_typedesc.length>0}}" 
        bindtap="lookMedicineService"
        class='detail-light-text detail-12-text detail-medicine-indications'
        style="display:flex;align-items:center">
        <view>
          {{dataSource.medicine_info.medicine_typedesc}}
        </view>
        <image lazy-load="{{true}}"  src="../../../images/wenhao.png" style="width:22rpx;height:22rpx;margin-left:10rpx"></image>
      </view>
    </view>
  </view>
  <view class='detail-line'></view>
  <!-- 运费、库存 -->
  <view class='detail-content-view'>
    <view class='detail-row-view detail-margin-bottom-15'>
      <view class='detail-light-text detail-13-text detail-margin-right-20 detail-min-width-26'>运费</view>
      <view class='detail-dark-text detail-13-text detail-margin-right-10'>
        {{dataSource.medicine_logistics.logisyics_amount}}</view>
      <view class='detail-logistics-line detail-margin-right-10'></view>
      <view class='detail-dark-text detail-13-text detail-margin-right-10'>{{dataSource.medicine_logistics.start_city}}
      </view>
      <view class='detail-image-title-view detail-dark-text detail-13-text'>
        <image lazy-load="{{true}}"  src='/images/YFWGoodsDetailModule/goods_deail_fly.png' class='detail-service-icon' mode='widthFix'>
        </image>
        {{dataSource.medicine_logistics.end_city}}
      </view>
    </view>
    <view class='detail-row-view'>
      <view class='detail-light-text detail-13-text detail-margin-right-20 detail-min-width-26'>库存</view>
      <view class='detail-dark-text detail-13-text detail-margin-right-10'>{{dataSource.medicine_inventory.inventory}}
      </view>
      <view wx:if="{{dataSource.medicine_inventory.limitation>0}}"
        class='detail-yellow-text detail-13-text detail-margin-right-10'>
        ({{dataSource.limit_buy_prompt}})</view>
      <!-- <view wx:if="{{dataSource.medicine_info.medicine_period_to.length>0}}" class='detail-normal-text detail-13-text detail-margin-right-10'>有效期至</view> -->
      <view wx:if="{{dataSource.medicine_info.medicine_period_to.length>0}}" class='detail-dark-text detail-13-text'>
        {{dataSource.medicine_info.medicine_period_to}}</view>
    </view>
  </view>
  <view class='detail-line'></view>
  <!-- 商品参数 -->
  <view class='detail-content-view'>
    <view class='detail-row-view'>
      <view class='detail-light-text detail-13-text detail-margin-right-20 detail-min-width-26'>参数</view>
      <view class='detail-column-view' style="padding-bottom:0rpx">
        <view class='detail-row-view'>
          <view class='detail-medicine-params-title detail-normal-text detail-13-text'>
            {{dataSource.medicine_info.medicine_authorizetionTitle}}</view>
          <view class=' detail-dark-text detail-13-text detail-margin-bottom-10'>
            {{dataSource.medicine_info.medicine_authorizetion}}</view>
        </view>
        <view class='detail-row-view'>
          <view class='detail-medicine-params-title detail-normal-text detail-13-text'>规格</view>
          <view class=' detail-dark-text detail-13-text detail-margin-bottom-10'>
            {{dataSource.medicine_info.medicine_standard}}</view>
        </view>
        <view wx:if="{{paramFlag}}">
          <view class='detail-row-view'>
            <view class='detail-medicine-params-title detail-normal-text detail-13-text'>剂型/型号</view>
            <view class=' detail-dark-text detail-13-text detail-margin-bottom-10'>
              {{dataSource.medicine_info.medicine_dosage_form}}</view>
          </view>
          <view class='detail-row-view'>
            <view class='detail-medicine-params-title detail-normal-text detail-13-text'>生产企业</view>
            <view class=' detail-dark-text detail-13-text detail-margin-bottom-10'>
              {{dataSource.medicine_info.medicine_manufacturer}}</view>
          </view>
          <view wx:if='{{dataSource.medicine_info.bentrusted_name}}' class='detail-row-view'>
            <view class='detail-medicine-params-title detail-normal-text detail-13-text'>上市许可人</view>
            <view class=' detail-dark-text detail-13-text detail-margin-bottom-10'>
              {{dataSource.medicine_info.bentrusted_name}}</view>
          </view>
          <view wx:if='{{dataSource.medicine_info.medicine_period}}' class='detail-row-view'>
            <view class='detail-medicine-params-title detail-normal-text detail-13-text'>有效期</view>
            <view class=' detail-dark-text detail-13-text detail-margin-bottom-10'>
              {{dataSource.medicine_info.medicine_period}}</view>
          </view>
          <view wx:if='{{dataSource.medicine_info.medicine_waring}}'
            class='detail-image-title-view detail-yellow-text detail-12-text'>
            <image lazy-load="{{true}}"  src='/images/YFWGoodsDetailModule/goods_deail_vacation.png' class='detail-service-icon'
              mode='widthFix'></image>
            {{dataSource.medicine_info.medicine_waring}}
          </view>
          <view wx:if='{{dataSource.medicine_info.medicine_vacation}}'
            class='detail-image-title-view detail-yellow-text detail-12-text'>
            <image lazy-load="{{true}}"  src='/images/YFWGoodsDetailModule/goods_deail_vacation.png' class='detail-service-icon'
              mode='widthFix'></image>
            {{dataSource.medicine_info.medicine_vacation}}
          </view>
          <view wx:if='{{dataSource.medicine_info.medicine_notice}}'
            class='detail-image-title-view detail-light-text detail-12-text'>
            <image lazy-load="{{true}}"  src='/images/YFWGoodsDetailModule/goods_deail_notice.png' class='detail-service-icon'
              mode='widthFix'></image>
            {{dataSource.medicine_info.medicine_notice}}
          </view>
        </view>
      </view>
      <view bindtap="changeParamFlag" class="openAndClose">
        <text style="padding-right:10rpx">展开</text>
        <image lazy-load="{{true}}"  src="/images/down_arrow.png" style="height:12rpx;width:24rpx;" class="{{paramFlag ? 'trans':''}}">
        </image>
      </view>
    </view>
  </view>
  <view class='detail-line'></view>
  <!-- 优惠券、促销 -->
  <!-- <view wx:if="{{dataSource.medicine_coupons.length>0 || dataSource.medicine_show_discount.length>0}}" class='detail-coupon-discount-view'>
    <view wx:if="{{dataSource.medicine_coupons.length>0}}" class="detail-coupon-content-view" bindtap="showCouponModal">
      <view class='detail-light-text detail-13-text detail-margin-right-20 detail-min-width-26 detail-coupon-bottom'>领券</view>
      <scroll-view class='detail-content-start detail-coupon-itemview'>
        <block wx:for="{{dataSource.medicine_coupons}}" wx:key="index">
          <view class='detail-discount-coupon'>{{item.name}}</view>
        </block>
      </scroll-view> 
      <image lazy-load="{{true}}"  src='/images/around_detail_icon.png' class='detail-detail-icon detail-coupon-bottom'></image>
    </view>
    <view wx:if="{{dataSource.medicine_show_discount.length>0}}" class="detail-discount-content-view" bindtap="showDiscountModal">
      <view class='detail-light-text detail-13-text detail-margin-right-20 detail-min-width-26 detail-margin-top-10'>促销</view>
      <view class='detail-column-view'>
        <block wx:for='{{dataSource.medicine_show_discount}}' wx:key='index'>
          <view class="detail-row-view detail-content-start detail-margin-top-10">
            <view wx:if="{{item.type==0}}" class='detail-discount-title detail-discount-back-yellow'>满减</view>
            <view wx:else class='detail-discount-title detail-discount-back-blue'>包邮</view>
            <view class='detail-dark-text detail-13-text detail-discount-des'>{{item.title}}</view>
          </view>
        </block>
      </view>
      <image lazy-load="{{true}}"  src='/images/around_detail_icon.png' class='detail-detail-icon detail-margin-top-10'></image>
    </view>
  </view>
  <view wx:if="{{dataSource.medicine_coupons.length>0 || dataSource.medicine_show_discount.length>0}}" class='detail-line'></view> -->
  <!-- 单品、套餐、多件装 -->
  <view class='detail-package-content-view'>
    <view class='detail-package-title-view' bindtap='showCommboModal' id="addCart">
      <view class='detail-light-text detail-13-text detail-margin-right-20 detail-min-width-26'>选择</view>
      <view class='detail-dark-text detail-13-text detail-width-300'>{{firstShowFlag ? dataSource.medicine_selectInfo.name:dataSource.medicine_selectInfo.modalName}}</view>
      <image lazy-load="{{true}}"  src='/images/around_detail_icon.png' class='detail-detail-icon'></image>
    </view>
    <view class='detail-package-view'>
      <block wx:for="{{dataSource.medicine_style.medicine_packages}}" wx:key="index">
        <view class='detail-package-item' bindtap='showCommboModal' id="addCart">{{item.name}}</view>
      </block>
    </view>
  </view>
  <view class='detail-line'></view>
  <!-- 商家信息 -->
  <view class='detail-content-view'>
    <view class='detail-row-view '>
      <image lazy-load="{{true}}"  src='{{dataSource.medicine_store.store_logo}}' class='detail-store-logo' mode='aspectFit'></image>
      <view class='detail-column-view'>
        <view class='detail-row-view detail-content-between'>
          <view class='detail-dark-text detail-15-text detail-bold-text detail-store-name'>
            {{dataSource.medicine_store.store_name}}</view>
          <view class='detail-row-view detail-content-center' bindtap='showStoreModal'>
            <view class='detail-13-text detail-yellow-subtitle'>查看资质</view>
            <image lazy-load="{{true}}"  src='/images/YFWGoodsDetailModule/goods_deail_arrow_yellow.png' class='detail-detail-icon-red'>
            </image>
          </view>
        </view>
        <view class='detail-row-view detail-content-start'>
          <block wx:for="{{dataSource.medicine_store.store_score_images}}" wx:key="index">
            <image lazy-load="{{true}}"  src='{{item}}' class='detail-store-star'></image>
          </block>
        </view>
      </view>
    </view>
    <scroll-view class='detail-store-medicine-view' scroll-x>
      <block wx:for="{{dataSource.medicine_store.store_recommend_medicine}}" wx:key="index">
        <view class='detail-store-medicine-item' bindtap='lookMedicineDetail' data-item='{{item}}'>
          <view class='detail-store-medicine-item-view'>
            <image lazy-load="{{true}}"  src='{{item.intro_image}}' class='detail-store-medicine-image'></image>
            <view class='detail-store-medicine-name'>{{item.medicine_name}}</view>
            <view class='detail-store-medicine-price'>
              <priceView price="{{item.price}}" fontSize="22"></priceView>
            </view>
          </view>
        </view>
      </block>
    </scroll-view>
    <view class='detail-row-view detail-store-button-view'>
      <view class='detail-store-button-back'>
        <view class='detail-store-button-all detail-row-view' bindtap='lookStoreAllMedicine'>
          <image lazy-load="{{true}}"  src='/images/YFWGoodsDetailModule/goods_deail_all.png' class='detail-store-button-icon'></image>
          <view class='detail-green-text detail-15-text detail-bold-text'>全部商品</view>
        </view>
      </view>
      <view class='detail-store-button-back detail-row-view detail-store-button-store' bindtap='lookStore'>
        <image lazy-load="{{true}}"  src='/images/YFWGoodsDetailModule/goods_deail_store.png' class='detail-store-button-icon'></image>
        <view class='detail-white-text detail-15-text detail-bold-text'>进入店铺</view>
      </view>
    </view>
  </view>
  <view class='detail-line'  id="goodsQuestion"></view>
  <!-- 常见问题-->
  <view class='detail-content-view' bindtap='lookAllQuestion'>
    <view class='detail-row-view detail-content-between'>
      <view class='detail-dark-text detail-15-text'>常见问题({{dataSource.medicine_question}})</view>
      <view class='detail-row-view detail-content-center'>
        <view class='detail-13-text detail-red-subtitle '>查看全部</view>
        <image lazy-load="{{true}}"  src='/images/YFWGoodsDetailModule/goods_deail_arrow_red.png' class='detail-detail-icon-red'></image>
      </view>
    </view>
  </view>
  <view class='detail-line-1'></view>
  <!-- 顾客评论  -->
  <view class='detail-content-view' bindtap='lookAllComment'>
    <view class='detail-row-view detail-content-between'>
      <view class='detail-dark-text detail-15-text'>顾客评论({{dataSource.medicine_comment.count}})</view>
      <view class='detail-row-view detail-content-center'>
        <view class='detail-13-text detail-red-subtitle '>查看全部</view>
        <image lazy-load="{{true}}"  src='/images/YFWGoodsDetailModule/goods_deail_arrow_red.png' class='detail-detail-icon-red'></image>
      </view>
    </view>
    <view>
      <block wx:for="{{dataSource.medicine_comment.comment_list}}" wx:key='index'>
        <view class='detail-margin-top-20'>
          <view class='detail-row-view detail-content-between'>
            <view class='detail-row-view detail-content-center'>
              <image lazy-load="{{true}}"  src='{{item.intro_image}}' class='detail-comment-icon'></image>
              <view class='detail-10-text detail-normal-text'>{{item.account_name}}</view>
            </view>
            <view class='detail-light-text detail-10-text'>{{item.create_time}}</view>
          </view>
          <view class='detail-dark-text detail-13-text detail-margin-top-10'>{{item.content}}</view>
        </view>
      </block>
    </view>
  </view>
  <view class='detail-line' id="goodsBaseInfo"></view>
  <!-- 基本信息、说明书、服务保障 悬浮占位 -->
  <view hidden='{{!dataSource.isSuspensionBaseInfo}}' style='opacity:0;'>
    <view class='detail-base-service'>
      <block wx:for="{{dataSource.medicine_detail}}" wx:key="index">
        <view wx:if="{{item.isShow == 1}}" class="detail-base-service-item" data-index='{{index}}'
          bindtap='changeGoodsDetailInfo'>
          <titleView title='{{item.title}}' fontWeight="bold" showLine="{{index==dataSource.medicine_detail_index}}" />
        </view>
      </block>
    </view>
    <view wx:if="{{dataSource.medicine_detail[dataSource.medicine_detail_index].notice.length>0}}"
      class='detail-base-notice detail-12-text'>{{dataSource.medicine_detail[dataSource.medicine_detail_index].notice}}
    </view>
  </view>
  <!-- 基本信息、说明书、服务保障 -->
  <view class="{{dataSource.isSuspensionBaseInfo ? 'detail-base-servic-fixed' : ''}}">
    <view class='detail-base-service'>
      <block wx:for="{{dataSource.medicine_detail}}" wx:key="index">
        <view wx:if="{{item.isShow == 1}}" class="detail-base-service-item" data-index='{{index}}'
          bindtap='changeGoodsDetailInfo'>
          <titleView title='{{item.title}}' fontWeight="bold" largeStyle="{{index==dataSource.medicine_detail_index}}"
            showLine="{{index==dataSource.medicine_detail_index}}" />
        </view>
      </block>
    </view>
    <view wx:if="{{dataSource.medicine_detail[dataSource.medicine_detail_index].notice.length>0}}"
