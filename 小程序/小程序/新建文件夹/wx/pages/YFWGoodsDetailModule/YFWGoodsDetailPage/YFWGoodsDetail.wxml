        <image lazy-load="{{true}}"  src='/images/YFWGoodsDetailModule/goods_deail_arrow_left.png' class='modal-direction-image'></image>
      </view>
      <swiper class='modal-store-swipper' current="{{dataSource.medicine_store.store_modal.index}}"
        bindchange='aptitudeChangeIndex'>
        <block wx:for="{{dataSource.medicine_store.store_modal.image_list}}" wx:key="index">
          <swiper-item>
            <image lazy-load="{{true}}"  src='{{item.image_url}}' class='modal-sipper-image' mode='aspectFit'
              bindtap='qualificationModalItemClick' data-images='{{dataSource.medicine_store.store_modal.image_list}}'
              data-current='{{item.image_url}}' data-type='qualification'></image>
          </swiper-item>
        </block>
      </swiper>
      <view wx:if="{{!dataSource.medicine_store.store_modal.isShowRight}}" class='modal-store-direction'></view>
      <view wx:else class='modal-store-direction' bindtap='changeAptitudeIndex' id="arrow_right">
        <image lazy-load="{{true}}"  src='/images/YFWGoodsDetailModule/goods_deail_arrow_right.png' class='modal-direction-image'></image>
      </view>
    </view>
    <view class='modal-store-bottom' bindtap='hideStoreModal'>关闭</view>
  </view>
</view>
<!-- loading -->
<view hidden='{{!isLoading}}' class='modal' catchtouchmove='true'>
  <view class='modal-loading-content'>
    <image lazy-load="{{true}}"  src='/images/loading_cycle.gif' class='modal-loding'></image>
  </view>
</view>

<view>
  <authentication id='authentication' />
</view>