.container{
  width: 100vw;
  height: 100vh;
  background-color: #fafafa
}
.warn-view {
  width: 100%;
  height: 68rpx;
  background-color: #faf8dc;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between

}
.warn-title {
  margin-left: 26rpx;
  font-size: 24rpx;
  color: #feac4c;
}
.open-notice {
  margin-right: 26rpx;
  display: flex;
  flex-direction: column;
  align-items: center
}
.open-notice text {
  font-size: 24rpx;
  color: #1fdb9b;
}
.line {
  background-color: #1fdb9b;
  height: 1rpx;
  width: 80rpx;
  margin-top: 10rpx;
}
.current-view {
  margin-left: 26rpx;
  margin-right: 26rpx;
  margin-top: 40rpx;
  padding: 38rpx 34rpx;
  border-radius: 14rpx;
  background-color: #f5f5f5;
  display: flex;
  flex-direction: row;

}
.price-title {
  font-size: 26rpx;
  color: #333;
}
.price {
  font-size: 26rpx;
  color: #ff3300;
}
.context-view {
  margin-left: 26rpx;
  margin-right: 26rpx;
  margin-top: 28rpx;
  padding: 36rpx 34rpx;
  border-radius: 14rpx;
  background-color: #ffffff;
  display: flex;
  flex-direction: row;
  align-items: center
}
.q-title {
  font-weight: bold
}
.input-view {
  font-size: 26rpx;
  font-weight: bold;
  flex: 1;
}
.place-view {
  font-size: 26rpx;
  color: #ccc;
}
.d-view {
  margin-left: 10rpx;
  margin-right: 10rpx;
}
.d-view image {
  width: 32rpx;
  height: 32rpx;
}
.msg-text {
  font-size: 26rpx;
}
.next-warn {
  font-size: 24rpx;
  color: #999;
  margin-top: 16rpx;
  margin-left: 60rpx;
  margin-right: 60rpx;
}
.confirmButton {
  height: 84rpx;
  background-image: linear-gradient(270deg, #00c891 0%, #1fdb9b 100%), linear-gradient(#fff, #fff);
  background-blend-mode: normal, normal;
  box-shadow: 0rpx 7rpx 15rpx 0rpx rgba(31, 219, 155, 0.5);
  border-radius: 42rpx;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 60rpx;
  margin-left: 26rpx;
  margin-right: 26rpx
}

.confirmButton text {
  font-size: 34rpx;
  color: #fff;
}