/* pages/YFWStoreModule/YFWShopDetailAllGoodsListVC.wxss */
.container{
  width: 100vw;
  height: 100vh;
}
.topSearchView{
  display: flex;
  flex-direction: row;
  background-color: #49ddb8;
  height: 100rpx;
  align-items: center;
}
.topSearchInputView{
  height: 68rpx;
  width: 70vw;
  border-radius: 34rpx;
  display: flex;
  flex-direction: row;
  margin-left: 40rpx;
  background-color: white;
  align-items: center;
}
.searchIcon{
  margin-left: 20rpx;
  width: 32rpx;
  height: 32rpx;
}
.topViewSearchInput{
  display: block;
  height: 68rpx;
  width: 70vw;
  margin-left: 20rpx;
  margin-right: 34rpx;
  background-color: white;  
}
.inputplaceholder{
  color:#cccccc;
  font-size: 30rpx;
}
.topViewSearchBotton{
  display: flex;
  height: 100rpx;
  width: 20vw;
  margin-left: 10rpx;
  margin-right: 40rpx;
  flex-direction: row;
  justify-content: flex-end;
  align-items: center
}
.searchBottonText{
  color: white;
  font-size: 34rpx;
}

