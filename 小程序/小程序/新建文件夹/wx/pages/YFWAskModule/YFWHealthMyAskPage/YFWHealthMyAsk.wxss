/* pages/YFWAskModule/myAskAns/myAskAns.wxss */
.topView {
  display: flex;
  width: 100vw;
  height: 100rpx;
  position: fixed;
  z-index: 99;
  top: 0rpx;
  background-color: #49ddb8;
  flex-direction: row;
  justify-content: space-between

}
.topSearchView {
  margin: auto 26rpx 22rpx 22rpx;
  height: 68rpx;
  flex: 1;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  display: flex;
}
.search_input_view {
  background-color: white;
  margin-left: 0rpx;
  margin-right: 22rpx;
  height: 68rpx;
  display: flex;
  flex: 1;
  border-radius: 34rpx;
  flex-direction: row;
  align-items: center;
}
.search_icon {
  width: 28rpx;
  height: 30rpx;
  margin-left: 18rpx;
  display: flex;
}
.placeholder {
  color: #cccccc;
  font-size: 28rpx;
  margin-left: 10rpx;
  display: flex;
  max-width: 70%;
  overflow: hidden; 
  text-overflow: ellipsis;
  white-space: nowrap; 
}
.search-text {
  margin-right: 0rpx;
  width: 92rpx;
  height: 42rpx;
  display: flex;

}
.search-text text {
  margin-left: 24rpx;
  color: #fff;
  font-size: 28rpx;
}
.myFlex {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
}

.noStation {
  height: 65%;
}

.title {
  margin-top: 100rpx;
  padding-top: 70rpx;
  padding-bottom: 58rpx;
  flex-direction: row;
  align-items: center;
  display: flex;
}

.hedPort {
  width: 120rpx;
  height: 120rpx;
  margin-left: 46rpx;
}

.telRght {
  margin-left: 36rpx;
  flex-direction: column;
}

.isname {
  font-weight: 600;
  font-size: 32rpx;
  color: #333333;
  margin-bottom: 9rpx;
}

.isname, .isQuion {
  font-size: 24rpx;
  margin-top: 9rpx;
  color: #999999;
}


page {
  background-color: #f8f8f8;
  height: 97%;
}

.myAuto {
  width: 100%;
  text-align: center;
}

.noQ {
  font-size: 24rpx;
  color: #999;
  width: 100%;
}

.noImg {
  width: 33%;
}
.newQ-All {
  background-color:#fff;
  margin-top:14rpx;
  padding-top: 20rpx;

}
.health-top-view {
  height: 68rpx;
  width: 100vw;
  display: flex;
  flex-direction: row;
  background-color: white;
  white-space: nowrap;
  -webkit-overflow-scrolling: touch;
}
.select-text {
  display: inline-flex;
  width:150rpx;
  padding-left:46rpx;
  margin-right:13rpx;
  justify-content: center
}
.line-view {
  margin-left: 46rpx;
  margin-right: 46rpx;
  height: 2rpx;
  background-color: #f5f5f5
}