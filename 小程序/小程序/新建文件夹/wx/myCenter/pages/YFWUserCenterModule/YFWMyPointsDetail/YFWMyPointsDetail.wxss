page{
  width: 100%;
  background-color: #fafafa;
  position: relative;
  overflow: hidden;
}
.bgc{
  width:100%;
  height:300rpx ;
}
.rank{
  position: absolute;
  top: 0;
  width: 100%;
  height: 300rpx;
  display: flex;
  align-items: center;
  flex-direction: column;
}
.rank_title{
  padding-top: 30rpx;
  font-size: 16px;
	color: #f5f5f5;
}
.rank_val{
  font-size: 40px;
  color: #fffaa5;
  padding: 18rpx;
}
.rank_frooze{
  font-size: 13px;
	color: #f5f5f5;
}
.rank_section{
  height: 102rpx;
  background-color: white;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.rank_list_title{
  display: flex;
  justify-content: space-between;
  color: #999999;
  height: 68rpx;
  line-height: 68rpx;
}
.rank_list_value{
  display: flex;
  justify-content: space-between;
  height: 68rpx;
  line-height: 68rpx;
}
.under {
  width: 130%;
  height: 12rpx;
  margin-top: -9rpx;
  background: linear-gradient(rgb(0,200,145), rgb(31,219,155));
  margin-left: -15%;
}
.explain_txt{
  width: auto;
  color: "#333333";
  font-size: 32rpx;
  font-weight: 600;
  display: inline-block;
}
.rank_list_title_name{
  text-align: center;
  font-size: 26rpx;
  width: 25%;
  color: #999999;
}
.getMore{
  height: 90rpx;
  line-height: 90rpx;
  color: #1fdb9b;
  text-align: center;
}
.noMore{
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  color:#1fdb9b;
  font-weight: bolder;
}